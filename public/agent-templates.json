      "id": "compute-broker",
      "name": "Compute Broker Agent",
      "description": "Aggregate GPU compute prices across providers and route jobs to the cheapest option.",
      "category": "brokerage",
      "complexity": "medium",
      "commands": ["price [gpu-model]", "route [job-spec]", "compare", "status [job-id]"],
      "integrations": ["Vast.ai API", "TensorDock API", "RunPod API", "Akash Network"],
      "nodes": [
        { "id": "trigger", "type": "event", "label": "User Request", "x": 100, "y": 150 },
        { "id": "query", "type": "task", "label": "Query Providers", "x": 300, "y": 150 },
        { "id": "compare", "type": "task", "label": "Compare Prices", "x": 500, "y": 150 },
        { "id": "confirm", "type": "gateway", "label": "User Confirm?", "x": 700, "y": 150 },
        { "id": "provision", "type": "task", "label": "Provision & Monitor", "x": 900, "y": 100 },
        { "id": "cancel", "type": "event", "label": "Cancelled", "x": 900, "y": 200 }
      ],
      "edges": [
        { "source": "trigger", "target": "query" },
        { "source": "query", "target": "compare" },
        { "source": "compare", "target": "confirm" },
        { "source": "confirm", "target": "provision", "label": "Yes" },
        { "source": "confirm", "target": "cancel", "label": "No" }
      ],
      "swimlanes": [
        { "id": "user", "label": "User" },
        { "id": "broker", "label": "Compute Broker" }
      ],
      "agentConfig": {
        "agentId": "compute-broker",
        "capabilities": ["price-compare", "job-routing", "provider-query"],
        "inputSchema": { "type": "object", "properties": { "gpu": { "type": "string" }, "jobSpec": { "type": "object" } } },
        "outputSchema": { "type": "object", "properties": { "provider": { "type": "string" }, "price": { "type": "number" }, "jobId": { "type": "string" } } }
      }
    },
